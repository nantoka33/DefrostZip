  <ItemGroup>
    <Content Include="bin\Debug\net8.0-windows\DefrostZip.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SharpCompress" Version="0.40.0" />
  </ItemGroup>

</Project>